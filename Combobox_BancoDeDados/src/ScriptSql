CREATE DATABASE  IF NOT EXISTS `teste` /*!40100 DEFAULT CHARACTER SET latin1 */;
USE `teste`;
CREATE TABLE  empresa_teste 
(id INT AUTO_INCREMENT,
 cargo VARCHAR(100), 
PRIMARY KEY (id));

